       <div class="container-fluid">

        <div style="background-color: #facb3d;" class="row">
            <div class="col-xs-4 col-sm-4 col-xs-offset-4 col-sm-offset-4 ">
                <img style="width: 100%;" src="/india/images/logo/paalogo.png">
            </div>
        </div>
        <div style="margin-top: 10px;" class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
               <ul class="list-group">
                   <li class="list-group-item active ">
                     <span style="font-size: medium; " class="badge">&#8377; 0 </span>
                      Account Balance
                   </li>
                </ul>
           </div>
        </div>           
         
         <div style="margin-top: 5px;" class="row">
             <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <ul class="list-group">
                    <li class="list-group-item active ">
                      <span style="font-size: medium; " class="badge">&#8377; 0 </span>
                        Daliy Balance
                    </li>
                    <li class=" list-group-item list-group-item-info ">
                        <span class="badge">&#8377; 0</span>
                        Daily Earnings
                      </li>
                    <li class=" list-group-item list-group-item-info ">
                        <span class="badge">&#8377; 0</span>
                        Daily Incetive
                       
                      </li>
                      <li class=" list-group-item list-group-item-info ">
                        <span class="badge">&#8377; 0</span>
                        Daily Cash collections
                       
                      </li>
                </ul>                
            </div>
         </div>


         <div class="row">
             
             <%if( 1 > 0){%> 
            <div class="col-xs-12 col-sm-12">                
                <button type="button" class="btn btn-success btn-block col-xs-12 col-sm-12">Deposit &#8377; 0</button>                
            </div>
            <%}else{%>
            <div class="col-xs-12 col-sm-12"> 
                <button type="button" class="btn btn-warning btn-block col-xs-12 col-sm-12">Withdrawal &#8377; 0</button>               
                               
            </div>
            <%}%>
        </div>

        
        <div style="margin-top: 20px;" class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <%if(1==0){%> 
                <a href="../india/preDrv"><button  type="button" class="btn btn-danger col-xs-4 col-sm-4 col-xs-offset-4 col-sm-offset-4">Go to Offline</button> </a>              
                <%}else{%> 
                <a href="../india/preDrv"><button  type="button" class="btn btn-danger  col-xs-4 col-sm-4 col-xs-offset-4 col-sm-offset-4">Back</button>  </a>              
                <%}%>
            </div>
        </div>
        
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" >
            <div class="form-group col-xs-8 col-sm-8">
                <label>Select Month</label>
                <!-- <select id="month" class="form-control">
                    <option value="0">January</option>
                    <option value="1">February</option>
                    <option value="2">March</option>
                    <option value="3">April</option>
                    <option value="4">May</option>
                    <option value="5">June</option>
                    <option value="6">July</option>
                    <option value="7">August</option>
                    <option value="8">September</option>
                    <option value="9">October</option>
                    <option value="10">November</option>
                    <option value="11">December</option>
                </select> -->

                <input type="date" class="form-control">
            </div>
            <div class="form-group col-xs-4 col-sm-4">
                <label style="color: #FFF;">&nbsp;g</label>
                <button onclick="monthlyhourcalculetion()" class="btn btn-primary ">Submit</button> 
            </div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12" style="overflow-x: scroll;" >
                <ul class="list-group">
                    <li class="list-group-item active ">
                        19-10-2020
